macro statements:
    whitespace, 'var',
    $identifier,
    whitespace, '=',
    $expression,
    whitespace, ';',
    $statements,
-> (context, name, value, next) => {
    context.declare_variable(name);
    context.assign_variable(name, value.execute(context));
    next.execute(context);
};

import 'addition';

export var result = x + 3;
